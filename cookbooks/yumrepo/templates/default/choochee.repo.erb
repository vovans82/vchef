[choochee]
name=ChooChee latest build
<% if node[:app_environment] == "qa" %>
baseurl=http://repo:Re90Pa23@repo.choochee.com/auth/qa/
<% elsif node[:roles].include?("backoffice") %>
baseurl=http://repo:Re90Pa23@repo.choochee.com/auth/latest/
<% elsif node[:app_environment] == "beta" %>
baseurl=http://repo:Re90Pa23@repo.choochee.com/auth/beta/
<% elsif node[:app_environment] == "prod" %>
baseurl=http://repo:Re90Pa23@repo.choochee.com/auth/prod/
<% elsif node[:app_environment] == "preprod" %>
baseurl=http://repo:Re90Pa23@repo.choochee.com/auth/preprod/
<% elsif node[:app_environment] == "ukinf" %>
baseurl=http://repo:Re90Pa23@repo.choochee.com/auth/prod/
<% elsif node[:hostname] =~ /rvsand\d+/ %>
baseurl=http://repo:Re90Pa23@repo.choochee.com/auth/prod/
<% else %>
baseurl=http://repo:Re90Pa23@repo.choochee.com/auth/latest/
<% end %>
enabled=1
gpgcheck=0
priority=1

[choochee-custom]
name=ChooChee Custom RPM's
<% if node[:app_environment] == "ops" %>
