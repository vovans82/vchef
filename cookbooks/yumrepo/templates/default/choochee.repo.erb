<% elsif node[:app_environment] == "staging" %> 
baseurl=http://repo002.rs1.inf.choochee.com/qa
<% else %>
baseurl=http://repo:Re90Pa23@repo.choochee.com/auth/custom/
<% end %>
enabled=1
gpgcheck=0
priority=1

[choochee-freeswitch]
name=ChooChee FreeSWITCH RPM's
baseurl=http://repo:Re90Pa23@repo.choochee.com/auth/freeswitch/latest
enabled=1
gpgcheck=0
priority=1

<% if node.roles.include?("global_proxy") %>
[choochee-opensips-17]
